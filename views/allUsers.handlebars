<div class="card-container">

  {{#each allUsers}}
  
    <div class="card">
  
        <div class="username">
  
          <h1>{{this.dataValues.name}}</h1>
  
        </div>
  
  
        <div class="buttons">
  
          <a href=""><button class="delete-btn">deletar</button></a>
  
          <a href="update-user/{{this.dataValues.id}}"><button class="save-btn">atualizar</button></a>
  
        </div>
  
    </div>
  
  {{/each}}
  
</div>
